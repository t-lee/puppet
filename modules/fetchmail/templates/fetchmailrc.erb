<% @fetchmail_data.account.each_pair do |key, value_hash| -%>
key = <% key -%> 
  'server' = '<%= value_hash['server'] %>',
  'user' = '<%= value_hash['user'] %>',
  'pass' = '<%= value_hash['pass'] %>',
  'opt' = '<%= value_hash['opt'] %>',
poll <% server -%> with proto POP3
       user '<% user -%>' there with password '<% pass -%>' is 'tommy@neutral-zone.de' here options <% opt -%> ;

<% end -%>

